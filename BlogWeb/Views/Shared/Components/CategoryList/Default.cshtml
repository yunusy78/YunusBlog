@model List<object>

<div class="tech-btm">
    <h4>Categories</h4>
    <div class="progress">
        <div class="progress-bar progress-bar-striped bg-warning" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0"
             aria-valuemax="100"></div>
    </div>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            var name = ((dynamic)item).Name;
            var count = ((dynamic)item).Count;

            <li class="list-group-item d-flex justify-content-between align-items-center">
                @name <span class="badge badge-primary badge-pill">@count</span>
            </li>
        }
    </ul>
</div>

